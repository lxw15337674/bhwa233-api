{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": "swc",
    "typeCheck": false,
    "deleteOutDir": true,
    "assets": ["assets/**/*"],
    "plugins": [
      "@nestjs/swagger"
    ]
  },
  "generateOptions": {
    "spec": false
  }
}